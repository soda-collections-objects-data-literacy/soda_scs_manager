{% set entity_type = entity_type|replace({'soda_scs_': ''}) %}
<div class="container">
  {% if entitiesByUser is not empty %}
    {% for username, entities in entitiesByUser %}
      <h2>{{ entity_type }}s of User {{ username }}</h2>
      <div {{ attributes }}>
        {% for entity in entities %}
          {{ entity }}
        {% endfor %}
      </div>
    {% endfor %}
  {% else %}
    <p>No {{ entity_type}}s found.</p>
  {% endif %}
</div>
